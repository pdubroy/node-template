{
  "name": "",
  "version": "0.1.0",
  "author": "Patrick Dubroy <pdubroy@gmail.com> (http://dubroy.com)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "browserify": "^13.0.1",
    "eslint": "^3.2.2",
    "eslint-config-google": "^0.6.0",
    "nodemon": "^1.9.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "build": "browserify . $npm_package_browserify_opts",
    "watch": "watchify -v . $npm_package_browserify_opts",
    "test": "tape test/*.js | tap-spec",
    "test-watch": "nodemon --exec 'npm test'",
    "prepublish": "npm run build && npm run test && npm run lint",
    "lint": "eslint index.js lib test"
  },
  "browserify": {
    "opts": "-d -s PACKAGE_NAME -o dist/PACKAGE_NAME.js"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  }
}
